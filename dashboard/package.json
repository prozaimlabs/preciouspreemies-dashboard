{
    "name": "dashboard",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "npm run clean --scripts-prepend-node-path && env-cmd -f env/.env.${ENVIRONMENT:-local} next build",
        "clean": "rimraf .next out",
        "deploy": "bash scripts/publish.sh",
        "dev": "env-cmd -f env/.env.${ENVIRONMENT:-local}  next dev",
        "start": "env-cmd -f env/.env.${ENVIRONMENT:-local}  next start",
        "style:all": "tsc && npm run style:lint && npm run style:prettier",
        "style:lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "style:prettier": "prettier --check '**/*.{js,jsx,ts,tsx}'",
        "make-pretty": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "prepare": "husky install"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.0.2",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "env-cmd": "^10.1.0",
        "lucide-react": "^0.338.0",
        "next": "13.5.6",
        "react": "^18",
        "react-dom": "^18",
        "tailwind-merge": "^2.2.1",
        "tailwindcss-animate": "^1.0.7"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10",
        "eslint": "^8",
        "eslint-config-next": "13.5.6",
        "husky": "^9.0.11",
        "postcss": "^8",
        "prettier": "^3.2.5",
        "tailwindcss": "^3",
        "typescript": "^5"
    }
}
